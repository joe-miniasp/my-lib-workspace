{
  "name": "my-lib-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "link:demo-form": "npm link dist/demo-form",
    "unlink:demo-form": "npm unlink dist/demo-form",
    "lib:build": "ng build demo-form --prod",
    "lib:serve": "ng build demo-form --watch",
    "lib:build:all": "npm run lib:build && npm run lib:schematics:build && npm run lib:schematics:postbuild:all",
    "lib:publish": "npm run lib:build:all && npm publish ./dist/demo-form",
    "lib:schematics:build": "node_modules/.bin/tsc -p ./projects/demo-form/tsconfig.schematics.json",
    "lib:schematics:copy:collection": "cd ./projects/demo-form && cp schematics/collection.json ../../dist/demo-form/schematics/collection.json",
    "lib:schematics:copy:ng-add:schemas": "cd ./projects/demo-form && cp --parents schematics/*/schema.json ../../dist/demo-form/",
    "lib:schematics:postbuild:ng-add": "npm run lib:schematics:copy:ng-add:schemas && npm run lib:schematics:copy:collection",
    "lib:schematics:copy:ng-generate:schemas": "cd ./projects/demo-form && cp --parents schematics/*/*/schema.json ../../dist/demo-form/",
    "lib:schematics:copy:ng-generate:files": "cd ./projects/demo-form && cp --parents -p schematics/**/**/files/** ../../dist/demo-form/ -r",
    "lib:schematics:postbuild:ng-generate": "npm run lib:schematics:copy:ng-generate:schemas && npm run lib:schematics:copy:ng-generate:files && npm run lib:schematics:copy:collection",
    "lib:schematics:postbuild:all": "npm run lib:schematics:postbuild:ng-add && npm run lib:schematics:postbuild:ng-generate",
    "lib:schematics:test:ng-add": "npm run lib:build && npm run lib:schematics:build && npm run lib:schematics:postbuild:ng-add && schematics ./dist/demo-form:ng-add --project=test-schematics --dry-run=true",
    "lib:schematics:test:ng-generate:loginForm": "npm run lib:build && npm run lib:schematics:build && npm run lib:schematics:postbuild:ng-generate && schematics ./dist/demo-form:loginForm --project=test-schematics --dry-run=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
